<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:background="@drawable/bg_image"
    android:focusableInTouchMode="true"
    tools:context="com.mega.matrimony.Activities.RegisterFirstActivity">

    <ScrollView
        android:id="@+id/ragi_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/lay_first"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dp"
                android:visibility="visible">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="10dp"
                    android:visibility="gone"
                    android:text="or"
                    android:textColor="@color/black"
                    android:textSize="15sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/btn_male"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/round_background"
                        android:drawablePadding="0dp"
                        android:gravity="center"
                        android:layout_marginRight="3dp"
                        android:paddingLeft="30dp"
                        android:paddingRight="36dp"
                        android:text="Male"
                        android:textAllCaps="false"
                        android:textColor="@color/White" />

                    <Button
                        android:id="@+id/btn_female"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/radio_unselected"
                        android:drawablePadding="0dp"
                        android:gravity="center"
                        android:paddingLeft="30dp"
                        android:paddingRight="36dp"
                        android:layout_marginLeft="3dp"
                        android:text="Female"
                        android:textAllCaps="false"
                        android:textColor="@color/black" />

                </LinearLayout>

                <EditText
                    android:id="@+id/et_f_name"
                    style="@style/editText_ragister"
                    android:layout_width="match_parent"
                    android:hint="Enter First Name*"
                    android:inputType="text"
                    android:maxLines="1"
                    android:paddingEnd="35dp"
                    android:paddingRight="35dp" />

                <EditText
                    android:id="@+id/et_l_name"
                    style="@style/editText_ragister"
                    android:layout_width="match_parent"
                    android:hint="Enter Last Name*"
                    android:inputType="text"
                    android:maxLines="1"
                    android:paddingEnd="35dp"
                    android:paddingRight="35dp" />

                <EditText
                    android:id="@+id/et_email"
                    style="@style/editText_ragister"
                    android:layout_width="match_parent"
                    android:hint="Email*"
                    android:inputType="textEmailAddress"
                    android:maxLines="1"
                    android:paddingEnd="35dp"
                    android:paddingRight="35dp" />

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/pass_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:hintAnimationEnabled="false"
                    app:hintEnabled="false"
                    app:passwordToggleEnabled="true">

                    <EditText
                        android:id="@+id/et_password"
                        style="@style/editText"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:background="@drawable/background_with_shadow"
                        android:drawableStart="@drawable/password_pink"
                        android:drawableLeft="@drawable/password_pink"
                        android:drawablePadding="10dp"
                        android:hint="Password*"
                        android:inputType="textPassword"
                        android:maxLength="30"
                        android:maxLines="1"
                        android:paddingStart="20dp"
                        android:paddingLeft="20dp"
                        android:paddingEnd="35dp"
                        android:paddingRight="35dp" />

                </android.support.design.widget.TextInputLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.rilixtech.CountryCodePicker
                        android:id="@+id/spin_code"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="0.40"
                        android:background="@drawable/background_with_shadow"
                        android:gravity="center"
                        app:ccp_defaultNameCode="IN" />

                    <EditText
                        android:id="@+id/et_mobile"
                        style="@style/editText_ragister"
                        android:layout_width="0dp"
                        android:layout_weight=".60"
                        android:hint="Mobile Number*"
                        android:inputType="numberDecimal"
                        android:maxLength="12"
                        android:maxLines="1" />

                </LinearLayout>

                <EditText
                    android:id="@+id/et_dob"
                    style="@style/editText_ragister"
                    android:layout_width="match_parent"
                    android:clickable="true"
                    android:focusable="false"
                    android:hint="Date of Birth*"
                    android:inputType="none"
                    android:maxLines="1"
                    android:paddingEnd="35dp"
                    android:paddingRight="35dp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/background_with_shadow"
                    android:paddingEnd="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:padding="10dp"
                        android:src="@drawable/religion_pink" />

                    <android.support.v7.widget.AppCompatSpinner
                        android:id="@+id/spin_religion"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingStart="37dp"
                        android:paddingLeft="37dp"
                        app:backgroundTint="@color/red" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/background_with_shadow"
                    android:paddingEnd="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:padding="10dp"
                        android:src="@drawable/caste_pink" />

                    <android.support.v7.widget.AppCompatSpinner
                        android:id="@+id/spin_caste"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingStart="37dp"
                        android:paddingLeft="37dp"
                        app:backgroundTint="@color/red" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/background_with_shadow"
                    android:paddingEnd="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:padding="10dp"
                        android:src="@drawable/tongue_pink" />

                    <android.support.v7.widget.AppCompatSpinner
                        android:id="@+id/spin_tongue"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingStart="37dp"
                        android:paddingLeft="37dp"
                        app:backgroundTint="@color/red" />
                </RelativeLayout>

                <Button
                    android:id="@+id/btn_first_submit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/round_effect"
                    android:text="Continue"
                    android:textAllCaps="false"
                    android:textColor="@color/White" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="20dp"
                    android:text="@string/lbl_all_rights_reserved"
                    android:textColor="@color/black" />

                <Button
                    android:id="@+id/btn_login"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/border_gradian"
                    android:text="Already a member? Log in"
                    android:textAllCaps="false"
                    android:textColor="@color/black" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lay_second"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="15dp"
                    android:text="Thanks for Registering. Now Let's Build your Profile." />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/register_divider_gray" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/country_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_country"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/state_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_state"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/city_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_city"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout

                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/heart_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_mari"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/lay_t_child"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp"
                        android:visibility="gone">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/dob_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_t_child"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/lay_status_child"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp"
                        android:visibility="gone">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/dob_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_child_status"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <Button
                        android:id="@+id/btn_second_submit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/round_effect"
                        android:text="Continue"
                        android:textAllCaps="false"
                        android:textColor="@color/White" />

                    <Button
                        android:id="@+id/btn_second_prev"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/round_effect"
                        android:text="Previous"
                        android:textAllCaps="false"
                        android:textColor="@color/White" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lay_third"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="15dp"
                    android:text="Add Your Education Informatation to make Stronger Your Profile." />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/register_divider_gray" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/edu_pink" />

                        <com.mega.matrimony.Custom.MultiSelectionSpinner
                            android:id="@+id/spin_edu"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/user_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_emp_in"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/money_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_income"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/user_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_occupation"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/desi_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                            android:id="@+id/spin_designation"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp"
                            app:backgroundTint="@color/red" />
                    </RelativeLayout>

                    <Button
                        android:id="@+id/btn_third_submit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/round_effect"
                        android:text="Continue"
                        android:textAllCaps="false"
                        android:textColor="@color/White" />

                    <Button
                        android:id="@+id/btn_third_prev"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/round_effect"
                        android:text="Previous"
                        android:textAllCaps="false"
                        android:textColor="@color/White" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lay_four"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
