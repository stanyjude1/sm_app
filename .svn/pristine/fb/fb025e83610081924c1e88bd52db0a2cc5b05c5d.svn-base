                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/user_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                        app:backgroundTint="@color/red"
                            android:id="@+id/spin_no_sis"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_with_shadow"
                        android:paddingEnd="10dp"
                        android:paddingRight="10dp">

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:padding="10dp"
                            android:src="@drawable/user_pink" />

                        <android.support.v7.widget.AppCompatSpinner
                        app:backgroundTint="@color/red"
                            android:id="@+id/spin_no_mari_sis"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="37dp"
                            android:paddingLeft="37dp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_with_shadow">

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:src="@drawable/about_pink" />

                        <EditText
                            android:id="@+id/et_about_family"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_gravity="top"
                            android:background="@null"
                            android:gravity="top"
                            android:textColor="@color/black"
                            android:hint="Write About Your Family"
                            android:inputType="textMultiLine"
                            android:paddingRight="35dp"
                            android:paddingLeft="40dp"
                            android:paddingTop="10dp" />
                    </RelativeLayout>

                    <Button
                        android:id="@+id/btn_family"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/round_effect"
                        android:text="Update"
                        android:textAllCaps="false"
                        android:textColor="@color/White" />

                </LinearLayout>

            </LinearLayout>

        </android.support.constraint.ConstraintLayout>

    </android.support.v4.widget.NestedScrollView>

</android.support.design.widget.CoordinatorLayout>