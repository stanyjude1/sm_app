<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="user"
            type="com.mega.matrimony.OtherUser" />
    </data>
    <LinearLayout

        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/main_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f4f3f3"
        android:orientation="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.mega.matrimony.Activities.ViewMyProfileActivity"
        tools:showIn="@layout/activity_view_my_profile">

        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@drawable/gradient_button"
                android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                app:layout_scrollFlags="scroll|enterAlways"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

        </android.support.design.widget.AppBarLayout>

        <android.support.v4.widget.NestedScrollView
            android:id="@+id/scroll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <android.support.constraint.ConstraintLayout
                android:descendantFocusability="blocksDescendants"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/img_profile"
                    android:layout_width="410dp"
                    android:layout_height="410dp"
                    android:scaleType="fitCenter"
                    android:background="@color/white"
                    android:src="@drawable/placeholder"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_photo_count"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/round_white"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text=""
                    android:translationZ="5dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/img_profile"
                    app:layout_constraintEnd_toEndOf="parent" />

                <TabHost
                    android:id="@+id/tabHost"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/img_profile">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <View android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/lighter_gray"/>

                        <TabWidget
                            android:id="@android:id/tabs"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <FrameLayout
                            android:id="@android:id/tabcontent"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <LinearLayout
                                android:id="@+id/tab1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:id="@+id/lay_main"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="8dp"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@color/White"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:background="@drawable/bottom_line_gray"
                                            android:orientation="horizontal">

                                            <TextView
                                                android:id="@+id/tv_basic_label"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:padding="15dp"
                                                android:text=" Basic Details"
                                                android:textColor="@color/red"
                                                android:textSize="16sp"
                                                android:textStyle="bold" />

                                        </LinearLayout>

                                        <LinearLayout
                                            android:id="@+id/lay_basic"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <RelativeLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:id="@+id/nam_label"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Name" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/nam_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.name}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </RelativeLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <RelativeLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:id="@+id/dob_label"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Marital Status" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/dob_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.marital_status}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </RelativeLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <RelativeLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:id="@+id/mari_label"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Mother Tongue" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/mari_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.m_tongue}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </RelativeLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <RelativeLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:id="@+id/loc_label"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Languages Known" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/loc_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.language}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </RelativeLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Height" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.height}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Weight" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/loc_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.weight}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Birthdate" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.dob}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Age" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.age}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>


                                            </LinearLayout>

                                        </LinearLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/White"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:background="@drawable/bottom_line_gray"
                                            android:orientation="horizontal">

                                            <TextView
                                                android:id="@+id/tv_reli_label"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:padding="15dp"
                                                android:text=" Religious Information"
                                                android:textColor="@color/red"
                                                android:textSize="16sp"
                                                android:textStyle="bold" />

                                        </LinearLayout>

                                        <LinearLayout
                                            android:id="@+id/lay_reli"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:visibility="gone">

                                            <LinearLayout
                                                android:layout_width="match_parent"

                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <RelativeLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:id="@+id/reli_label"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Religion" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/reli_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.religion}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </RelativeLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <RelativeLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:padding="5dp">

                                                    <TextView
                                                        android:id="@+id/caste_label"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Caste" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_below="@+id/caste_label"
                                                        android:padding="5dp"
                                                        android:text="@{user.caste}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </RelativeLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"

                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Subcaste" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.subcaste}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Manglik" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.manglik}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Star" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.star}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Horoscope" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.horoscope}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Gothra" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="@{user.gothra}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Moonsign" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.moonsign}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                            </LinearLayout>

                                        </LinearLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/White"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:background="@drawable/bottom_line_gray"
                                            android:orientation="horizontal">

                                            <TextView
                                                android:id="@+id/tv_pro_disc_label"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:padding="15dp"
                                                android:text=" About Me &amp; Hobby"
                                                android:textColor="@color/red"
                                                android:textSize="16sp"
                                                android:textStyle="bold" />

                                        </LinearLayout>

                                        <LinearLayout
                                            android:id="@+id/lay_about"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:visibility="gone">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="About Us" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="@{user.about}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                                <RelativeLayout
                                                    android:layout_width="1dp"
                                                    android:layout_height="match_parent"
                                                    android:background="#ccc" />

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="5dp"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

                                                    <TextView

                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"
                                                        android:text="Hobby" />

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:padding="5dp"

                                                        android:text="@{user.hobby}"
                                                        android:textColor="@color/black"
                                                        android:textSize="15sp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:background="@drawable/bottom_line_gray"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_weight="1"
                                                    android:orientation="vertical"
                                                    android:padding="5dp">

